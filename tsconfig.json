{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@images/*": ["src/assets/images/*"],
      "@layouts/*": ["src/layouts/*"],
      "@css/*": ["src/css/*"],
      "@components/*": ["src/components/*"],
      "@scripts/*": ["src/scripts/*"],
      "@pages/*": ["src/pages/*"]
    }
  }
}